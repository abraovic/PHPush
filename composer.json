{
    "name": "abraovic/phpush",
    "type": "library",
    "description": "Send push notifications to iOS and Android by PHP",
    "homepage": "https://github.com/abraovic/PHPush",
    "license": "Apache-2.0",
    "keywords": ["push", "apns", "gcm"],
    "authors": [
        {
            "name": "Ante Braovic",
            "email": "abraovic@gmail.com",
            "homepage": "http://antebraovic.me"
        }
    ],
    "support": {

        "source": "https://github.com/abraovic/phpush"
    },
    "require": {
        "php": ">=5.3",
        "lib-curl": "*",
        "symfony/yaml": ">=2.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "autoload": {
        "psr-4": {"abraovic\\PHPush\\": "src/"}
